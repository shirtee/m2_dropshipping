<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Shirtee_Dropshipping::shirtee" title="Shirtee" sortOrder="1">
                    <resource id="Shirtee_Dropshipping::dropshipping" title="Dropshipping" sortOrder="1">
                    	<resource id="Shirtee_Dropshipping::products" title="Products" sortOrder="1"/>
                    	<resource id="Shirtee_Dropshipping::orders" title="Orders" sortOrder="2"/>
                    	<resource id="Shirtee_Dropshipping::settings" title="Settings" sortOrder="3"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Shirtee_Dropshipping::config" title="Shirtee" translate="title" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
